{
  "name": "convert-lion-modules",
  "version": "0.0.0",
  "scripts": {
    "test": "npm run test:node",
    "test:node": "mocha test/*.test.node.js",
    "test:watch": "onchange 'src/**/*.js' 'test/**/*.js' -- npm run test --silent",
    "storybook": "start-storybook -p 9001 --config-dir ./demo/.storybook --root-dir ../../",
    "storybook:build": "build-storybook --stories \"../calendar/stories/*.stories.{js,mdx}\" --output-dir ./storybook-static"
  },
  "dependencies": {
    "@open-wc/demoing-storybook": "^1.6.1"
  },
  "devDependencies": {
    "@lion/core": "0.4.0",
    "@lion/calendar": "0.6.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "onchange": "^6.1.0"
  }
}
